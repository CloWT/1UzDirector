<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uz.a1uz.a1uzdirector">

    <uses-permission android:name="android.permission.INTERNET" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/uz_launcher_ic"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppThemess">
        <activity android:name=".Activity.Login_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.Main_Activity"
            android:configChanges="orientation|screenSize" />
        <activity android:name=".Activity.TablesActivitys.BankTables.AccountsTable" />
        <activity android:name=".Activity.TablesActivitys.BankTables.BankBankReport" />
        <activity android:name=".Activity.TablesActivitys.BankTables.BankPeriodTable" />
        <activity android:name=".Activity.TablesActivitys.ProceedsTables.ProceedsTable" />
        <activity android:name=".Activity.TablesActivitys.ProceedsTables.ProceedsPeriodTable" />
        <activity android:name=".Helpers.DatePeriodPicker" />
        <activity android:name=".Activity.TablesActivitys.StoreTables.StoreTable" />
        <activity android:name=".Activity.TablesActivitys.StoreTables.StorePeriodTable" />
        <activity android:name=".Activity.TablesActivitys.CreditTables.CreditTable" />
        <activity android:name=".Activity.TablesActivitys.CreditTables.CreditSecondReport" />
        <activity android:name=".Activity.TablesActivitys.DebitTables.DebitTable" />
        <activity android:name=".Activity.TablesActivitys.DebitTables.DebitSecondReport" />
        <activity
            android:name=".Activity.TablesActivitys.CurrentTables.CurrentConsumptionTable"
            android:label="@string/title_activity_current_consumption_table"
            android:parentActivityName=".Activity.Main_Activity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="uz.a1uz.a1uzdirector.Activity.Main_Activity" />
        </activity>
        <activity android:name=".Activity.TablesActivitys.CurrentTables.CurrentConsumptionSecondTable"></activity>
    </application>

</manifest>